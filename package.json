{
  "name": "bitcoin-testnet3-transfer-site",
  "version": "0.8.2",
  "private": true,
  "scripts": {
    "db:migrate": "knex migrate:latest",
    "bump": "CI=true sh ./scripts/build-prod.sh && bump --prompt --commit --tag --all",
    "lint": "eslint **/*.js",
    "start": "node ./bin/www",
    "start:dev": "nodemon --watch app.js --watch middleware --watch models --watch routes --exec node -- ./bin/www",
    "test": "NODE_ENV=test nyc --reporter=text --reporter=html mocha --recursive",
    "test:watch": "NODE_ENV=test mocha --recursive --watch"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^2.0.1",
    "bookshelf": "^0.13.3",
    "cookie-parser": "~1.4.3",
    "debug": "~3.1.0",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.14.6",
    "morgan": "~1.9.0",
    "nodemon": "^1.17.5",
    "pg": "^7.4.3",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.19.1",
    "eslint-config-harris": "^1.4.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.2.0",
    "supertest": "^3.1.0",
    "version-bump-prompt": "^4.1.0"
  }
}